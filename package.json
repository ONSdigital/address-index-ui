{
  "name": "address-index-ui",
  "version": "1.0.0",
  "description": "To run this project:",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test:jest": "node --experimental-vm-modules node_modules/.bin/jest",
    "lint": "npx prettier --write \"**/*.mjs\""
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.mjs$": "babel-jest"
    },
    "testMatch": [
      "**/tests/jest_js_tests/**/*.test.mjs"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "moduleNameMapper": {
      "^/static/(.*)$": "<rootDir>/src/aims_ui/static/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ONSdigital/address-index-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ONSdigital/address-index-ui/issues"
  },
  "homepage": "https://github.com/ONSdigital/address-index-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@eslint/js": "^9.3.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-portsoc": "^1.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "eslintConfig": {
    "extends": "portsoc",
    "root": true,
    "env": {
      "browser": true
    }
  },
  "dependencies": {
    "form-data": "^4.0.4",
    "jest-environment-jsdom": "^29.7.0",
    "micromatch": "^4.0.8"
  }
}
